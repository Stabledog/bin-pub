# ~/.gitsync-config-cygwin
# vim: filetype=sh :
# We're sourced by gitsync, and our job is to print
# a table of [dirname] [options]

function print_config {
    echo "
    ~/.vim 
    ~/bin 
    ~/bin/tox-py 
    ~/journal
    ~/my-home
    ~/trainsane
    ~/riddle-bin
    ~/riddle
    "

    [[ -f ~/.taskrc/gitsync/gitsync-local ]] && source ~/.taskrc/gitsync/gitsync-local

}

# Remove blanks:
print_config | sed 's/^ *//' | egrep -v '^$' | sort -u

