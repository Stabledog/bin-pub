# gitsync-config-base
# vim: filetype=sh :

function print_config {

    (
        cat 
        # read the local if it exists:
        if  [[ -f ~/.taskrc/gitsync/gitsync-local ]]; then
            cat ~/.taskrc/gitsync/gitsync-local
        fi

    ) |  sed 's/^ *//' | egrep -v '^$' | sort -u

}


