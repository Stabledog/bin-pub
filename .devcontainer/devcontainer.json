{
    "name": "bin-pub",
	"initializeCommand": "${localEnv:HOME}/bin/.devcontainer/bin/initializeCommand.sh",
	"dockerComposeFile": "docker-compose.yaml",
	"service": "bin-pub",
	"workspaceFolder": "/home/vscode/bin",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker"
			]
		}
	},
    "mounts": ["source=/var/run/docker.sock,target=/var/run/docker-host.sock,type=bind"],
	"remoteEnv": {
		"LOCAL_WORKSPACE_FOLDER": "${localWorkspaceFolder}"
	},
	"remoteUser": "vscode"
}
